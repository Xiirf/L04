{
    "extends": "airbnb",
     "env": {
        "browser": true,
        "es6": true
      },
      "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
      "jsx-a11y/label-has-for": 0,
      "linebreak-style": 0
    },
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
     "sourceType": "module",
     "parser": "babel-eslint"
     },
     "plugins": [
     "react"
    ]
  }